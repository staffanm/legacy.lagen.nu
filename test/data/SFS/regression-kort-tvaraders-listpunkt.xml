<Forfattning>
  <Paragraf id="P1" ordinal="1">
    <Stycke id="P1S1">
      <unicode>I ärenden som rör registrering ska avgifter betalas med följande belopp:</unicode>
      <NumreradLista>
        <Listelement id="P1S1N6" ordinal="6">
          <unicode>6. vid annan registreringsanmälan, 800 kronor. </unicode>
          <LinkSubject predicate="dct:references" uri="http://rinfo.lagrummet.se/publ/sfs/9999:999#L2008:24">Förordning (2008:24).</LinkSubject>
        </Listelement>
      </NumreradLista>
    </Stycke>
  </Paragraf>
  <Paragraf id="P2" ordinal="2">
    <Stycke id="P2S1">
      <unicode>Om en anmälan samtidigt omfattar registrering eller registrering av ändring av fler än en firma.</unicode>
    </Stycke>
  </Paragraf>
</Forfattning>